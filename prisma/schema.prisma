
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Camera {
  id Int @id @default(autoincrement())
  name String  @unique
  slug String @unique

}

model Brand {
  id Int @id @default(autoincrement())
  name String @unique
  slug String @unique

}

model Type {
  id Int @id @default(autoincrement())
  name String @unique
  slug String @unique

}